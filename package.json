{
  "name": "react-input-tags",
  "version": "3.0.2",
  "description": "React component for inputting tags",
  "main": "lib/index.js",
  "scripts": {
    "server": "start-storybook -p 9001 -c .storybook",
    "web": "open http://localhost:9001/",
    "lint": "eslint src/*",
    "pretest": "npm run lint",
    "test": "npm run test-mocha && npm run test-storyshots",
    "test-mocha": "mocha test/* --require babel-core/register --require test/setup.js",
    "test-storyshots": "storyshots",
    "test-storyshots-update": "storyshots -u",
    "prebuild": "npm run test && npm run remove-dir && npm run create-dir",
    "remove-dir": "rm -rf dist",
    "create-dir": "mkdir -p dist",
    "build": "npm run build-lib",
    "build-lib": "babel src --out-dir lib",
    "build-docs": "build-storybook -c .storybook -o docs",
    "predeploy": "npm run build",
    "deploy": "npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baldwmic/react-input-tags.git"
  },
  "author": "Michael Baldwin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baldwmic/react-input-tags/issues"
  },
  "homepage": "https://github.com/baldwmic/react-input-tags#readme",
  "peerDependencies": {
    "react": "^15.4.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storyshots": "^2.1.0",
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.8.2",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "jest": "^18.1.0",
    "jsdom": "^9.6.0",
    "mocha": "^3.1.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
